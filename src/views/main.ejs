<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
  <title>ResourceKraft IoT</title>
  <base href="/">

  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="icon" type="image/x-icon" href="favicon.ico">
  <link href="css/styles.css" rel="stylesheet">
  <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
  <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,700" rel="stylesheet">

</head>
<body>
	<div class="content" ng-controller="MainController" ng-cloak>
		<div class="group-top header">
			<div class="logo">IoT</div>
			<div class="content-header"></div>
		</div>
		<div class="group-middle container">
			<div class="menu-list">
				<div class="menu-header">
					Menu
				</div>
				<ul>
					<li class="menu-item">
						<a ng-href="#!/dashboard">Dashboard</a>
					</li>
					<li class="menu-item">
						<a ng-href="#!/thing">Things</a>
					</li>

					<li class="menu-item">
						<a ng-href="/logout">Logout</a>
					</li>
				</ul>
			</div>
			<div ng-view  class="main">

			</div>
		</div>
	</div>
	<script src="angular.min.js"></script>
	<script src="angular-route.min.js"></script>

	<!-- Controllers-->
	<script src="app/data.module.js"></script>
	<script src="app/thing.module.js"></script>
	<script src="app/app.js"></script>

	<script>
		var GLOBAL = { user : {user:"<%= user.user %>",name:"<%= user.name %>"} };
	</script>

</body>
</html>
